variant: fcos
version: 1.5.0
systemd:
  units:
    - name: var-opt-sabnzbd.mount
      enabled: true
      contents: | 
        [Unit]
        Requires=network-online.target
        [Mount]
        What=truenas.home.0n5.us:/mnt/scrapdata/sabnzbd
        Where=/var/opt/sabnzbd
        Type=nfs
        [Install]
        RequiredBy=docker.service
        WantedBy=multi-user.target
