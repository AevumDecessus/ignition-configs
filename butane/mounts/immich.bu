variant: fcos
version: 1.5.0
systemd:
  units:
    - name: var-opt-immich.mount
      enabled: true
      contents: | 
        [Unit]
        Requires=network-online.target
        [Mount]
        What=truenas.home.0n5.us:/mnt/tank1/immich
        Where=/var/opt/immich
        Type=nfs
        [Install]
        RequiredBy=docker.service
        WantedBy=multi-user.target
