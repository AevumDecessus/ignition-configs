variant: fcos
version: 1.5.0
passwd:
   users:
     - name: docker_user
       primary_group: docker_user
       no_user_group: true
       uid: 3001
       groups:
         - docker
         - video_media
         - misc
         - calibre
         - secure
       shell: /bin/bash
       home_dir: /var/opt/docker
       no_create_home: true
systemd:
  units:
    - name: docker.service
      enabled: true
