variant: fcos
version: 1.5.0
storage:
  files:
    - path: /etc/sysconfig/network-scripts/ifcfg-ens192
      mode: 0644
      overwrite: false
      append:
        - inline: |
            TYPE="Ethernet"
            NAME="ens192"
            DEVICE="ens192"
            ONBOOT=yes
            BOOTPROTO=none
            NETMASK="255.255.254.0"
            GATEWAY="192.168.202.1"
            DNS1="192.168.202.31"
