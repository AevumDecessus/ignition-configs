variant: fcos
version: 1.5.0
storage:
  files:
    - path: /etc/NetworkManager/system-connections/ens18.nmconnection
      mode: 0644
      overwrite: false
      contents:
        inline: |
          [connection]
          id=ens18
          type=ethernet
          interface-name=ens18
          [ipv4]
          dns=192.168.202.31;
          dns-search=home.0n5.us
          may-fail=false
          method=manual
